{
  "manifest_version": 3,
  "name": "BOQ 1-Click Auto Fill (One PC Lock)",
  "version": "1.2",
  "description": "Auto YES & -5 for e-GP with Device Locking",
  "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://www.eprocure.gov.bd/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service_worker.js"
  }
}