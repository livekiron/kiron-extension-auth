{
  "manifest_version": 3,
  "name": "BOQ Tool Protected",
  "version": "1.0",
  "description": "Email verification system for BOQ Tool",

  "permissions": [
    "storage"
  ],

  "host_permissions": [
    "https://kiron-extension-auth.vercel.app/*"
  ],

  "background": {
    "service_worker": "service_worker.js"
  },

  "action": {
    "default_popup": "popup.html"
  }
}
